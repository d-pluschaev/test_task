<div class="row">
    <div class="col-md-4">
        <h5 class="text-center"><strong>Extended search</strong></h5>

        <form id="search-form" class="form-horizontal">

            <div class="form-group">
                <label for="inputFirstName" class="col-sm-6 control-label text-center-imp">First name</label>
                <div class="col-sm-6">
                    <input name="firstName" type="text" class="form-control" id="inputFirstName" placeholder="Text Input">
                </div>
            </div>

            <div class="form-group">
                <label for="inputLastName" class="col-sm-6 control-label text-center-imp">Last name</label>
                <div class="col-sm-6">
                    <input name="lastName" type="text" class="form-control" id="inputLastName" placeholder="Text Input">
                </div>
            </div>

            <div class="form-group">
                <label for="inputBirthDate" class="col-sm-6 control-label text-center-imp">Birth date</label>
                <div class="col-sm-6">
                    <input name="birthday" type="text" class="form-control datepicker" data-date-format="dd.mm.yyyy"
                           id="inputBirthDate" placeholder="Date Picker Input" data-provide="datepicker">
                </div>
            </div>

            <div class="form-group">
                <label for="inputAge" class="col-sm-6 control-label text-center-imp">Age</label>
                <div class="col-sm-6">
                    <div id="dd-age" class="dropdown custom">
                        <button class="btn btn-default dropdown-toggle form-control" type="button"
                                id="inputAge" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                            <span id="dd-age-disp">-</span>
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu dropdown-menu-right" aria-labelledby="inputAge">
                            <li class="text-center" data-value=""><a href="#"> - </a></li>
                            {% for i in range(16, 99) %}
                                <li class="text-center" data-value="{{ i }}"><a href="#">{{ i }}</a></li>
                            {% endfor %}
                        </ul>
                    </div>

                    <input id="dd-age-val" value="" name="age" type="hidden" autocomplete="off">
                </div>
            </div>

            <div class="form-group">
                <label for="inputEmail" class="col-sm-6 control-label text-center-imp">Email</label>
                <div class="col-sm-6">
                    <input name="email" type="email" class="form-control" id="inputEmail" placeholder="Email input">
                </div>
            </div>

            <div class="form-group">
                <label for="inputHomeAddress" class="col-sm-6 control-label text-center-imp">Home Address</label>
                <div class="col-sm-6">
                    <input name="homeAddress" type="text" class="form-control" id="inputHomeAddress" placeholder="Text Input">
                </div>
            </div>

            <div class="form-group">
                <label for="inputCompanyAddress" class="col-sm-6 control-label text-center-imp">Company Address</label>
                <div class="col-sm-6">
                    <input name="companyAddress" type="text" class="form-control" id="inputCompanyAddress" placeholder="Text Input">
                </div>
            </div>

            <div class="form-group">
                <label for="inputPhone" class="col-sm-6 control-label text-center-imp">Phone</label>
                <div class="col-sm-6">
                    <input name="phone" type="text" class="form-control" id="inputPhone" placeholder="Masked Input">
                </div>
            </div>

            <div class="form-group">
                <label for="inputCompanyName" class="col-sm-6 control-label text-center-imp">Company Name</label>
                <div class="col-sm-6">
                    <input name="companyName" type="text" class="form-control" id="inputCompanyName" placeholder="Text Input">
                </div>
            </div>

            <div class="form-group">
                <label for="inputJobPosition" class="col-sm-6 control-label text-center-imp">Job Position</label>
                <div class="col-sm-6">
                    <input name="jobPosition" type="text" class="form-control" id="inputCV" placeholder="Text Input">
                </div>
            </div>

            <div class="form-group">
                <label for="inputCV" class="col-sm-6 control-label text-center-imp">CV</label>
                <div class="col-sm-6">
                    <input name="cv" type="text" class="form-control" id="inputCV" placeholder="Text Input">
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 text-center">
                    <button id="search-submit" class="btn btn-default submit" type="submit">FIND</button>
                </div>
                <div class="col-md-12 text-center">
                    <h4><i id="search-loader" class="loader" style="display: none"></i></h4>
                </div>
            </div>

        </form>
    </div>

    <script type="html/tpl" id="search-result-row-template">
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-6">
                        <div class="row">
                            <div class="col-md-5"><strong>Name:</strong></div>
                            <div class="col-md-7">{firstName} {lastName}</div>
                        </div>
                        <div class="row">
                            <div class="col-md-5"><strong>Birth date:</strong></div>
                            <div class="col-md-7">{birthday}</div>
                        </div>
                        <div class="row">
                            <div class="col-md-5"><strong>Age:</strong></div>
                            <div class="col-md-7">{age}</div>
                        </div>
                        <div class="row">
                            <div class="col-md-5"><strong>Email:</strong></div>
                            <div class="col-md-7">{email}</div>
                        </div>
                        <div class="row">
                            <div class="col-md-5"><strong>Home address:</strong></div>
                            <div class="col-md-7">{homeAddress}</div>
                        </div>
                        <div class="row">
                            <div class="col-md-5"><strong>Company:</strong></div>
                            <div class="col-md-7">{companyName}</div>
                        </div>
                        <div class="row">
                            <div class="col-md-5"><strong>Company address:</strong></div>
                            <div class="col-md-7">{companyAddress}</div>
                        </div>
                        <div class="row">
                            <div class="col-md-5"><strong>Job position:</strong></div>
                            <div class="col-md-7">{jobPosition}</div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div>{cv}</div>
                    </div>
                </div>
            </div>
        </div>
    </script>

    <div class="col-md-8">
        <div class="well well-sm">
            <h5 id="search-result-label" class="text-center">No results</h5>
            <p id="pagination-top" class="text-center"></p>
            <p id="search-results"></p>
            <p id="pagination-bottom" class="text-center"></p>
        </div>
    </div>
</div>
